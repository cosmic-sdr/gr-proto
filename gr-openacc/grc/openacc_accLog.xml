<block>
  <name>Acclog</name>
  <key>openacc_accLog</key>
  <category>[OPENACC]</category>
  <import>import openacc</import>
  <make>openacc.accLog($contextType, $deviceId, $nValue, $kValue, $vlen)</make>
  <param>
    <name>Contexttype</name>
    <key>contextType</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Deviceid</name>
    <key>deviceId</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Nvalue</name>
    <key>nValue</key>
    <type>float</type>
  </param>
  <param>
    <name>Kvalue</name>
    <key>kValue</key>
    <type>float</type>
  </param>
  <param>
    <name>Vector length</name>
    <key>vlen</key>
    <value>1</value>
    <type>int</type>
  </param>
  <check>$vlen &gt; 0</check>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
